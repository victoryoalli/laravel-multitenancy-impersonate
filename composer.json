{
    "name": "victoryoalli/laravel-multitenancy-impersonate",
    "description": "Laravel Multitenancy Impersonation from landlord to tenant",
    "keywords": [
        "victoryoalli",
        "laravel" ,
        "multitenancy",
        "impersonate"
    ],
    "homepage": "https://github.com/victoryoalli/multitenancy-impersonate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Victor Yoalli",
            "email": "victoryoalli@gmail.com",
            "homepage": "https://victoryoalli.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-multitenancy": "^2.1",
        "illuminate/auth":"^8.63",
        "illuminate/database":"^8.63",
        "illuminate/support":"^8.63"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2.1",
        "orchestra/testbench": "^6.21.1",
        "phpunit/phpunit": "^9.5.8",
        "psalm/plugin-laravel": "^1.5.1",
        "vimeo/psalm": "^4.10.0"
    },
    "autoload": {
        "psr-4": {
            "VictorYoalli\\MultitenancyImpersonate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VictorYoalli\\MultitenancyImpersonate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VictorYoalli\\MultitenancyImpersonate\\MultitenancyImpersonateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
